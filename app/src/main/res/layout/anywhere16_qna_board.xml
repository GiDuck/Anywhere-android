<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.androidtown.anywhere.any_16_0_qnaboard.QnaBoard">
    <RelativeLayout
        android:id="@+id/actionbar"
        android:layout_width="match_parent"
        android:layout_height="50dp" />

    <FrameLayout
        android:id="@+id/menubar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />


    <android.support.design.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/container1"
        android:layout_below="@id/actionbar">

        <android.support.v7.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/qnaBoard_recycler">
        </android.support.v7.widget.RecyclerView>

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/qnaBoard_writeFloatBtn"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginBottom="30dp"
            android:layout_marginRight="20dp"
            android:layout_gravity="bottom|end"
            android:src="@drawable/ic_edit_black_24dp"/>


        <android.support.design.widget.FloatingActionButton
            android:id="@+id/qnaBoard_searchFloatBtn"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginBottom="30dp"
            android:layout_marginRight="95dp"
            android:layout_gravity="bottom|end"
            app:backgroundTint="@color/anywhere_main_color3"
            android:src="@drawable/ic_search_black_24dp"
            />


    </android.support.design.widget.CoordinatorLayout>

</RelativeLayout>
